{
  "name": "qunit-assertions-extra",
  "version": "0.7.0",
  "description": "Collection of helpful assertions for use with qunit",
  "main": "dist/index.js",
  "keywords": [
    "qunit",
    "testing",
    "assertions",
    "assert",
    "extra"
  ],
  "repository": "https://github.com/NullVoxPopuli/qunit-assertions-extra",
  "license": "MIT",
  "author": "NullVoxPopuli",
  "private": false,
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "release": "release-it -n",
    "build": "rollup -c",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint . --ext=js,ts",
    "start": "ember serve",
    "test": "yarn test:unit && yarn test:ember",
    "test:unit": "jest",
    "test:ember": "./tests/run-tests.sh",
    "prepublish": "rollup -c"
  },
  "peerDependencies": {
    "qunit": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.20",
    "@types/qunit": "^2.9.0",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "babel-eslint": "^10.0.3",
    "babel-preset-env": "^1.6.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-ember": "^7.1.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "execa": "^3.2.0",
    "jest": "^24.9.0",
    "lerna-changelog": "^0.8.0",
    "prettier": "^1.18.2",
    "qunit": "^2.9.3",
    "release-it": "^12.4.3",
    "release-it-lerna-changelog": "^1.0.3",
    "rollup": "^1.26.0",
    "rollup-plugin-typescript2": "^0.25.1",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.4"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md"
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true
    }
  },
  "changelog": {
    "repo": "simplabs/qunit-dom",
    "labels": {
      "breaking": ":boom: Breaking Change",
      "enhancement": ":rocket: Enhancement",
      "bug": ":bug: Bug Fix",
      "documentation": ":memo: Documentation",
      "internal": ":house: Internal"
    }
  },
  "jest": {
    "testRegex": "(/__tests__/[^-]*)\\.[jt]sx?$",
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "volta": {
    "node": "12.13.0",
    "yarn": "1.19.1"
  }
}
